{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= name %>",
  "author": "",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"<% if (modules.includes('eslint')) { %>,
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn run lint"<% } %><% if (modules.includes('heroku-postbuild')) { %>,
