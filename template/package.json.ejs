  "dependencies": {
    "nuxt": "*"
  },
  "devDependencies": {
    "node-sass": "*",
    "sass-loader": "*"<% if (modules.includes('eslint')) { %>,
    "eslint": "*",
    "eslint-loader": "*",
    "eslint-plugin-vue": "*"<% if (modules.includes('babel')) { %>,
    "babel-eslint": "*"<% } %><% } %><% if (modules.includes('filters')) { %>,
    "vue2-filters": "*"<% } %><% if (modules.includes('headroom')) { %>,
    "vue-headroom": "*"<% } %><% if (modules.includes('i18n')) { %>,
    "nuxt-i18n": "*"<% } %>
  }
